<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/mx_ripple">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/mx_green_dim"/>
            <stroke
                android:width="1dp"
                android:color="@color/mx_green"/>
            <corners android:radius="8dp"/>
            <padding
                android:left="8dp"
                android:top="8dp"
                android:right="8dp"
                android:bottom="8dp"/>
        </shape>
    </item>
</ripple>